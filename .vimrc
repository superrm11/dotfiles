noremap i k
noremap k j
noremap j h
noremap ; i
noremap <CR> i
noremap I 5k
noremap K 5j
noremap J b
noremap L w
noremap <C-i> <C-u>
noremap <C-k> <C-d>
noremap <C-l> $
noremap <C-j> 0
nnoremap <C-w> :q<CR>
nnoremap <C-t> :vs<CR>
nnoremap <C-e> <C-w>w

set number
set splitbelow
set splitright
syntax on
set tabstop=4
